{
    "name": "visavi/motor",
    "description": "cms",
    "require": {
        "php": "^8.0.2",
        "ext-mbstring": "*",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "php-di/slim-bridge": "^3.3",
        "visavi/captcha": "^2.0",
        "visavi/motor-orm": "^3.3",
        "visavi/cleanup": "^1.5",
        "league/plates": "^3.5",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "filp/whoops": "^2.14",
        "symfony/var-dumper": "^6.0",
        "deployer/deployer": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "GPL-3.0-only"
}
