{
    "name": "visavi/motor",
    "description": "cms",
    "require": {
        "php": "^8.0.2",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^7.7",
        "intervention/image": "^2.7",
        "league/plates": "^3.5",
        "monolog/monolog": "^2.9",
        "php-di/slim-bridge": "^3.3",
        "shieldon/simple-cache": "^1.3",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "symfony/console": "^6.0",
        "symfony/mailer": "^6.0",
        "visavi/captcha": "^3.0",
        "visavi/cleanup": "^1.5",
        "visavi/motor-orm": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "filp/whoops": "^2.15",
        "symfony/var-dumper": "^6.0",
        "deployer/deployer": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "license": "GPL-3.0-only"
}
